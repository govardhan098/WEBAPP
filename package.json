{
  "name": "tech-prism-backend",
  "type": "module",
  "version": "1.0.0",
  "description": "Node js backend for Tech Prism app",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harireddy7/tech-prism.git"
  },
  "keywords": [
    "node.js",
    "e-commerce",
    "express.js",
    "tech-prism"
  ],
  "author": "Hari Kotha",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/harireddy7/tech-prism/issues"
  },
  "homepage": "https://github.com/harireddy7/tech-prism#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.7"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  }
